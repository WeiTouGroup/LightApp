doctype html
html
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible' content='IE=edge')
    meta(name='viewport' content="user-scalable=no, width=device-width, initial-scale=1, maximum-scale=1")
    for css in cssList
      link(rel='stylesheet', href=css)
    title=title
  body
    block content
  for js in jsList
    script(src=js)
